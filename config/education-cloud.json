{
  "orgName": "Edu Cloud Example",
  "edition": "Developer",
  "description": "Example settings for creating an Education Cloud scratch org. Customize features and permissions as needed for your project.",
  "features": [
    "EducationCloud:10",
    "AccountingSubledgerGrowthEdition",
    "AccountingSubledgerUser",
    "AnalyticsQueryService",
    "Assessments",
    "EnableSetPasswordInApi",
    "Fundraising",
    "IndustriesActionPlan",
    "IndustriesSalesExcellenceAddOn",
    "IndustriesServiceExcellenceAddOn",
    "LightningScheduler",
    "LightningServiceConsole",
    "MarketingUser",
    "OmniStudioDesigner",
    "OmniStudioRuntime",
    "PersonAccounts",
    "PublicSectorAccess"
  ],
  "settings": {
    "lightningExperienceSettings": {
      "enableS1DesktopEnabled": true
    },
    "mobileSettings": {
      "enableS1EncryptedStoragePref2": false
    },
    "chatterSettings": {
      "enableChatter": true
    },
    "apexSettings": {
      "enableDisableParallelApexTesting": true
    },
    "enhancedNotesSettings": {
      "enableEnhancedNotes": true
    },
    "industriesSettings": {
      "enableIndustriesAssessment": true,
      "enableDiscoveryFrameworkMetadata": true,
      "enableEducationCloud": true,
      "enableStudentSuccess": true,
      "enableAcademicOperations": true,
      "enableAlumniRelations": true,
      "enableCarePlansPreference": true,
      "enableBenefitManagementPreference": true,
      "enableBenefitAndGoalSharingPref": true
    },
    "objectSettings": {
      "assessmentQuestion": {
        "sharingModel": "Public Read Only",
        "defaultRecordType": "default"
      },
      "benefitType": {
        "sharingModel": "Public Read Only",
        "defaultRecordType": "default"
      },
      "program": {
        "sharingModel": "Public Read Only",
        "defaultRecordType": "default"
      },
      "programEnrollment": {
        "sharingModel": "Public Read Only",
        "defaultRecordType": "default"
      },
      "serviceResource": {
        "sharingModel": "Public Read Only",
        "defaultRecordType": "default"
      },
      "successTeam": {
        "sharingModel": "Public Read Only",
        "defaultRecordType": "default"
      },
      "unitOfMeasure": {
        "sharingModel": "Public Read Only",
        "defaultRecordType": "default"
      }
    }
  }
}
